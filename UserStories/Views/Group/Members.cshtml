@model IEnumerable<UserStories.Core.Models.MemberViewModel>

@{
    ViewBag.Title = "Members";
}

<h2>Members</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GroupCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StoryCount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GroupCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StoryCount)
        </td>
        
    </tr>
}

</table>
